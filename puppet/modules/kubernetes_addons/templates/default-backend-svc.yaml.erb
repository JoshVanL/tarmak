kind: Service
apiVersion: v1
metadata:
  labels:
    app: default-http-backend
    addonmanager.kubernetes.io/mode: Reconcile
  name: default-http-backend
  namespace: <%= @namespace %>
spec:
  ports:
    - protocol: TCP
      port: 80
      targetPort: 8080
  selector:
    app: default-http-backend
