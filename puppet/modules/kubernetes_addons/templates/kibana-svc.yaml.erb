apiVersion: v1
kind: Service
metadata:
  name: kibana-logging
  namespace: <%= @namespace %>
  labels:
    k8s-app: kibana-logging
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "Kibana"
    addonmanager.kubernetes.io/mode: Reconcile
spec:
  ports:
  - port: 5601
    protocol: TCP
    targetPort: ui
  selector:
    k8s-app: kibana-logging
